{
		"$schema": "https://vega.github.io/schema/vega-lite/v3.json",
		"data": {
			"url": "datasets/movies.csv"
		},
		"mark": {
			"type": "bar",
			"tooltip": null
		},
		"encoding": {
			"x": {
				"bin": true,
				"field": "Running_Time",
				"type": "quantitative",
				"axis": {
					"title": "Running_Time"
				}
			},
			"y": {
				"field": "Running_Time",
				"aggregate": "count",
				"type": "quantitative",
				"axis": {
					"title": "COUNT"
				}
			}
		}
	}