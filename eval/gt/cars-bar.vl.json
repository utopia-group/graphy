{
		"$schema": "https://vega.github.io/schema/vega-lite/v3.json",
		"data": {
			"url": "datasets/cars.csv"
		},
		"mark": {
			"type": "bar",
			"tooltip": null
		},
		"encoding": {
			"x": {
				"field": "Cylinders",
				"type": "nominal"
			},
			"y": {
				"aggregate": "mean",
				"field": "MPG",
				"type": "quantitative",
				"axis": {
					"title": "AVG (MPG)",
					"format": "~s"
				}
			}
		},
		"ignore_orientation": true
	}