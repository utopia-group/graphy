{
		"$schema": "https://vega.github.io/schema/vega-lite/v3.json",
		"data": {
			"url": "datasets/movies.csv"
		},
		"mark": {
			"type": "point",
			"filled": true,
			"opacity": 0.5,
			"tooltip": null
		},
		"encoding": {
			"x": {
				"field": "IMDB_Rating",
				"type": "quantitative"
			},
			"y": {
				"field": "Rotten_Tomatoes_Rating",
				"type": "quantitative"
			}
		},
		"ignore_orientation": true
	}